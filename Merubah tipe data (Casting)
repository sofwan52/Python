# Casting integer to other
angka_integer = 2
print(angka_integer) # => 2
data_float = float(angka_integer)
print(data_float) # => 2.0
data_string = str(angka_integer)
print(data_string) # => 2
data_boolean = bool(angka_integer)
print(data_boolean) # => true

# Casting float to other
angka_float = 2.5
print(angka_float) # => 2.5
data_int = int(angka_float)
print(data_int) # => 2
data_string = str(angka_float)
print(data_string) # => 2.5
data_boolean = bool(angka_float)
print(data_boolean) # => true

# Casting string to other
string = "kamu cantik"
print(string) # => Kamu cantik
data_int = int(string)
print(data_int) # => (Tidak bisa\error)
data_float = float(string)
print(data_float) # => (Tidak bisa\error)
data_boolean = bool(string)
print(data_boolean) # => true

# Casting boolean to other (if not 0 is true)
boolean = False
print(boolean) # => False
data_int = int(boolean)
print(data_int) # => 0
data_float = float(boolean)
print(data_float) # => 0,0
data_string = bool(boolean)
print(data_string) # => false
